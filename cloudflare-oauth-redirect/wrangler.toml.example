name = "zipline-oauth-redirect"
main = "src/worker.js"
compatibility_date = "2024-01-01"

# Replace with your Cloudflare account ID
account_id = "YOUR_CLOUDFLARE_ACCOUNT_ID"

# Route configuration - replace with your domain
# IMPORTANT: The path MUST be /app/oauth-redirect
# The domain should match your Zipline domain for default configuration
# Or use a different domain and build with --dart-define=OAUTH_REDIRECT_URL=<full-url>
routes = [
  { pattern = "your-domain.com/app/oauth-redirect", zone_name = "your-domain.com" }
]

[vars]
# Replace with your Zipline instance URL
ZIPLINE_URL = "https://your-zipline-instance.com"